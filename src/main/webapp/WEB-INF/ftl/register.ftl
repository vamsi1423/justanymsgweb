<form action="submitRegister.jam" method="post">
<table style="margin-left: 50px;margin-top: 50px;">
	<tr>
		<td class="label">
			First Name:
		</td>
		<td>
			<input class="text-field" type="text" name="firstName" size="15" />
		</td>
	</tr>
	<tr>
		<td class="label">
			Middle Initial:
		</td>
		<td>
			<input class="text-field" type="text" name="middleName" size="1" />
		</td>
		</tr>
		<tr>
		<td class="label">
			Last Name:
		</td>
		<td>
			<input class="text-field" type="text" name="lastName" size="15" />
		</td>
	</tr>
	<tr>
		<td class="label">
			User Name:
		</td>
		<td>
			<input class="text-field" type="text" name="userName" size="15" />
		</td>
	</tr>
	<tr>
		<td class="label">
			Password:
		</td>
		<td>
			<input class="text-field" type="password" name="password" size="15" />
		</td>
	</tr>
	<tr>
		<td class="label">
			Email Address:
		</td>
		<td>
			<input class="text-field" type="text" name="emailAddress" size="15" />
		</td>
	</tr>
	<tr>
		<td class="label">
			Mobile Number:
		</td>
		<td>
			<input class="text-field" type="text" name="mobileNumber" size="15" />
		</td>
	</tr>
</table>
<br />
<div align="left" style="margin-left: 250px;">
	<input class="button" type="submit" value="Register" />
</div>
</form>